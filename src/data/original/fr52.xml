<l xml:id="fr52_680.15"><gap reason="Fragmentverlust" extent="unbekannt"/></l>
<l xml:id="fr52_680.16"><gap reason="Fragmentverlust" extent="unbekannt"/></l>
<l xml:id="fr52_680.17"><gap reason="Fragmentverlust" extent="unbekannt"/></l>
<l xml:id="fr52_680.18"><gap reason="Fragmentverlust" extent="unbekannt"/></l>
<l xml:id="fr52_680.19"><gap reason="Fragmentverlust" extent="unbekannt"/></l>
<l xml:id="fr52_680.20"><gap reason="Fragmentverlust" extent="unbekannt"/></l>
<l xml:id="fr52_680.21"><gap reason="Fragmentverlust" extent="unbekannt"/></l>
<l xml:id="fr52_680.22"><gap reason="Fragmentverlust" extent="unbekannt"/></l>
<l xml:id="fr52_680.23"><gap reason="Fragmentverlust" extent="unbekannt"/></l>
<l xml:id="fr52_680.24"><gap reason="Fragmentverlust" extent="unbekannt"/></l>
