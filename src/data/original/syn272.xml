<l n="*m 272.06"><seg type="große_Variante" n="a2">e<corr type="Emendation">r</corr></seg> hiez bereiten <seg type="große_Variante" n="b2">im</seg> <seg type="große_Variante" n="c1">zwei</seg> bat.</l>
<l n="*m 272.07"><seg type="kleine_Variante" n="a1">dô</seg> lac vrouwe <name ref="regp:p0142" type="Person">Jeschute</name></l>
