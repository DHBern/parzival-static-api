<l n="*G 329.11">diu <corr type="Emendation">r</corr>î<corr type="Emendation">ch</corr>e, <corr type="Emendation">w</corr>î<corr type="Emendation">s</corr>e <name ref="regp:p0355" type="Person">heidenîn</name></l>
