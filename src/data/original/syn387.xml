<l n="*m 387.27">dô nam der herzoge <name ref="regp:p0026" type="Person">Astor</name></l>
<l n="*m 387.28"><name ref="regp:p0217" type="Person"><seg type="kleine_Variante" n="a2">Meliagan<corr type="Emendation">z</corr></seg></name> <seg type="große_Variante" n="a2">de</seg> <name ref="rego:o0101" type="Ort"><seg type="große_Variante" n="b1">Jamor</seg></name>.</l>
