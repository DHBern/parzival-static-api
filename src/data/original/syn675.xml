<l n="*m 675.19">ruof<corr type="Emendation">t</corr>, <seg type="kleine_Variante" n="a2">wan</seg> <seg type="große_Variante" n="a2">eim liep</seg> <seg type="große_Variante" n="b2">beschiht</seg>,</l>
