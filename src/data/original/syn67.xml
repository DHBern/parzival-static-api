<l n="*m 67.15"><name ref="regp:p0050" type="Person">Zidegast</name> <seg type="große_Variante" n="c2">de</seg> <name ref="rego:o0139" type="Ort"><seg type="große_Variante" n="d1">Logrois</seg></name></l>
