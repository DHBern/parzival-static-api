<l n="*m 731.05"><seg type="große_Variante" n="a1">mit</seg> ritterschaft <seg type="große_Variante" n="b1">ê</seg> <seg type="große_Variante" n="c1">dick<reg type="Synkope">e</reg></seg> <seg type="singuläre_Lesart">erranc</seg>,</l>
