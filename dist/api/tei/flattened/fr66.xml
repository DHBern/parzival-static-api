<pb xml:id="fr66001r"/>
         <cb xml:id="fr66001ra"/>
         <l xml:id="fr66_700.15">Vf ſtvͦnt Gahmvreterſ kint</l>
         <l xml:id="fr66_700.16">Er ſprach alle die hie ſint</l>
         <l xml:id="fr66_700.17">Sitzen ſtille un̄d helfen mir</l>
         <cb xml:id="fr66001rb"/>
         <l xml:id="fr66_701.29">Got lone dir dcaz div bivteſ ſtrit</l>
         <l xml:id="fr66_701.30">Eſt ab̉er nit fvr mich noch zit</l>
         <l xml:id="fr66_702.01">AArtvſ die bete horte</l>
         <pb xml:id="fr66001v"/>
         <cb xml:id="fr66001va"/>
         <l xml:id="fr66_703.11">Waſ ſin orſ gewappēnt un̄d ſin lip</l>
         <l xml:id="fr66_703.12">b geben richeloſiv wip</l>
         <l xml:id="fr66_703.13">Siner zimierde ſtivre</l>
         <cb xml:id="fr66001vb"/>
         <l xml:id="fr66_704.22"/>
         <l xml:id="fr66_704.23">E man frieſhe dcaz mere</l>
         <l xml:id="fr66_704.24">Daz da uer miſſet were</l>
         <l xml:id="fr66_704.25">
            <milestone unit="Absatz" type="Absatz_mit_Zeilenumbruch" subtype="Anfang"/>Parzifal deſ kvͦnen</l>
         <l xml:id="fr66_704.26">ab erc welle ſvͦnen<milestone unit="Absatz" type="Absatz_mit_Zeilenumbruch" subtype="Ende"/>
         </l>
         <pb xml:id="fr66002r"/>
         <cb xml:id="fr66002ra"/>
         <l xml:id="fr66_706.04"/>
         <l xml:id="fr66_706.05">Den kvͦnen wiganden</l>
         <l xml:id="fr66_706.06">Div orſ waren geſtanden</l>
         <l xml:id="fr66_706.07">Do ſtrittn ſuſ die werden</l>
         <cb xml:id="fr66002rb"/>
         <l xml:id="fr66_707.16">Herre</l>
         <l xml:id="fr66_707.17">Alſir mir g</l>
         <l xml:id="fr66_707.18">Do ir mich</l>
         <l xml:id="fr66_707.19">Nv rvͦw</l>
         <pb xml:id="fr66002v"/>
         <cb xml:id="fr66002va"/>
         <l xml:id="fr66_708.29">nt</l>
         <l xml:id="fr66_708.30">elpivnt</l>
         <l xml:id="fr66_709.01"> conertivre</l>
         <cb xml:id="fr66002vb"/>
         <l xml:id="fr66_710.11">Daz ir brvder der liebeſte man</l>
         <l xml:id="fr66_710.12">Den maget inz herze ie gewan</l>
         <l xml:id="fr66_710.13">Mit ain ander uehten ſolden</l>
         <l xml:id="fr66_710.14"/>
