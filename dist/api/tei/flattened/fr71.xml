<l xml:id="fr71_25.15">D<choice><am>̉</am><ex>er</ex></choice> wiſe dege<gap reason="Fragmentverlust" extent="unbekannt"/></l>
<l xml:id="fr71_25.16">ovch hat <gap reason="Fragmentverlust" extent="unbekannt"/></l>
<l xml:id="fr71_25.17">Cheilet vo<gap reason="Fragmentverlust" extent="unbekannt"/></l>
<l xml:id="fr71_25.18">vil mani<gap reason="Fragmentverlust" extent="unbekannt"/></l>
<l xml:id="fr71_25.19">Diͤ brahte <gap reason="Fragmentverlust" extent="unbekannt"/></l>
<l xml:id="fr71_25.20">d<choice><am>̉</am><ex>er</ex></choice> ſchotte<gap reason="Fragmentverlust" extent="unbekannt"/></l>
<l xml:id="fr71_25.21">Vn<choice><am>̄</am><ex>d</ex></choice> ſin ge<gap reason="Fragmentverlust" extent="unbekannt"/></l>
<l xml:id="fr71_25.22">mit ma<gap reason="Fragmentverlust" extent="unbekannt"/></l>
<l xml:id="fr71_25.23">Dort w<gap reason="Fragmentverlust" extent="unbekannt"/></l>
<l xml:id="fr71_25.24">da lît da<gap reason="Fragmentverlust" extent="unbekannt"/></l>
<l xml:id="fr71_25.25">Mit vliez<gap reason="Fragmentverlust" extent="unbekannt"/></l>
<l xml:id="fr71_25.26">offenlich<gap reason="Fragmentverlust" extent="unbekannt"/></l>
<l xml:id="fr71_25.27">Geſach ſ<gap reason="Fragmentverlust" extent="unbekannt"/></l>
<l xml:id="fr71_25.28">ſi mvͦſte<gap reason="Fragmentverlust" extent="unbekannt"/></l>
<l xml:id="fr71_25.29">Jr herz<gap reason="Fragmentverlust" extent="unbekannt"/></l>
<l xml:id="fr71_25.30">ſit and<choice><am>̉</am><ex>er</ex></choice></l>
<l xml:id="fr71_26.01"><seg type="Initiale" subtype="Lombarde" n="3" rend="Farbe: rot">D</seg>er gaſt<gap reason="Fragmentverlust" extent="unbekannt"/></l>
