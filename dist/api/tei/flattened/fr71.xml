<l xml:id="fr71_26.04">wa<gap reason="Fragmentverlust" extent="unbekannt"/></l>
<l xml:id="fr71_26.05">Zorn<gap reason="Fragmentverlust" extent="unbekannt"/></l>
<l xml:id="fr71_26.06">ir habt <gap reason="Fragmentverlust" extent="unbekannt"/></l>
<pb xml:id="fr71002v"/><cb xml:id="fr71002va"/>
<l xml:id="fr71_26.07"><gap reason="Fragmentverlust" extent="unbekannt"/>en.</l>
<l xml:id="fr71_26.08"><gap reason="Fragmentverlust" extent="unbekannt"/> ſchade<choice><am>̄</am><ex>n</ex></choice>.</l>
<l xml:id="fr71_26.09"><gap reason="Fragmentverlust" extent="unbekannt"/>ſin gert.</l>
<l xml:id="fr71_26.10"><gap reason="Fragmentverlust" extent="unbekannt"/>waz wert.</l>
<l xml:id="fr71_26.13"><gap reason="Fragmentverlust" extent="unbekannt"/></l>
<l xml:id="fr71_26.14"><gap reason="Fragmentverlust" extent="unbekannt"/>vht.</l>
<l xml:id="fr71_26.15"><gap reason="Fragmentverlust" extent="unbekannt"/><unclear reason="Zweifel">nn</unclear> ein wîp.</l>
<l xml:id="fr71_26.16"><gap reason="Fragmentverlust" extent="unbekannt"/>in lîp.</l>
<l xml:id="fr71_26.17"><gap reason="Fragmentverlust" extent="unbekannt"/>milt<choice><am>̉</am><ex>er</ex></choice> hant.</l>
<l xml:id="fr71_26.18"><gap reason="Fragmentverlust" extent="unbekannt"/></l>
