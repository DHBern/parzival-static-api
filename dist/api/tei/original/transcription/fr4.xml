         <l xml:id="fr4_368.02">h<choice>
               <am>̉</am>
               <ex>er</ex>
            </choice>re durh got <lb/> die rede lat.</l>
         <l xml:id="fr4_368.03">ſuſ ſp<choice>
               <am rend="hochgestellt">a</am>
               <ex>ra</ex>
            </choice>ch deſ <lb/> kunigiſ lothiſ ſun.</l>
         <l xml:id="fr4_368.04">durch <lb/> uw<choice>
