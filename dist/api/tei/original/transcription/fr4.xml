         <pb xml:id="fr4002v"/>
         <cb xml:id="fr4002va"/>
         <l xml:id="fr4_367.06">
            <gap reason="Fragmentverlust" extent="unbekannt"/>in <gap reason="Fragmentverlust" extent="unbekannt"/>
            <lb/>de.</l>
         <l xml:id="fr4_367.07">Jch han zwo tochtere <lb/> die mir ſint.</l>
         <l xml:id="fr4_367.08">lieb wa<choice>
