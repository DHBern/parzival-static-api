            <gap reason="Fragmentverlust" extent="unbekannt"/>
         </l>
         <l xml:id="fr27_330.23">Sit ich uf monſ<supplied reason="unleserlich" resp="Sekundärliteratur">auaſch</supplied>
            <gap reason="Fragmentverlust" extent="unbekannt"/>
         </l>
         <l xml:id="fr27_330.24">Daz mich von waren v<supplied reason="unleserlich" resp="Sekundärliteratur">r</supplied>
            <gap reason="unleserlich" extent="unbekannt"/>
         </l>
         <l xml:id="fr27_330.25">Ohteiz wie manig cla<supplied reason="unleserlich" resp="Sekundärliteratur">r</supplied>
