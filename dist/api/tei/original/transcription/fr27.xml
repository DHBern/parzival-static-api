         <l xml:id="fr27_330.10">E<supplied reason="unleserlich" resp="Sekundärliteratur">z iſt ein ſtrenge ſharf</supplied> ge<gap reason="Fragmentverlust" extent="unbekannt"/>
         </l>
         <l xml:id="fr27_330.11">Gein mir mit worten hie <gap reason="Fragmentverlust" extent="unbekannt"/>
         </l>
