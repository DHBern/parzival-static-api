         <l xml:id="fr27_328.18">Ich fuͦr daher durch <gap reason="unleserlich" extent="unbekannt"/>
         </l>
         <l xml:id="fr27_328.19">
            <subst hand="#sr">
               <del>D*</del>
               <add>Da</add>
            </subst> wold er gerne ir<gap reason="Fragmentverlust" extent="unbekannt"/>
         </l>
         <l xml:id="fr27_328.20">Die vart die ich her h<gap reason="Fragmentverlust" extent="unbekannt"/>
         </l>
         <l xml:id="fr27_328.21">Daz warb er do in m<gap reason="Fragmentverlust" extent="unbekannt"/>
         </l>
         <l xml:id="fr27_328.22">Sinir mvͦter muͦme<gap reason="Fragmentverlust" extent="unbekannt"/>
         </l>
         <l xml:id="fr27_328.23">Bin ich er iſt ein chu<gap reason="Fragmentverlust" extent="unbekannt"/>
         </l>
